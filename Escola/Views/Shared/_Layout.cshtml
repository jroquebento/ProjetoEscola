<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/layout.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.8.3.js"></script>
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Home", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Aluno", "Index", "Aluno", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Bimestre", "Index", "Bimestre", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Disciplina", "Index", "Disciplina", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Nota", "Index", "Nota", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Pessoa", "Index", "Pessoa", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Professor", "Index", "Professor", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("ProfessorDisciplina", "Index", "ProfessorDisciplina", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Usuário", "Index", "Usuario", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Boletim", "Index", "Boletim", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Gerar Boletim", "GerarBoletim", "Boletim", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                </ul>
            </div>
        </div>
    </div>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>